{
  "name": "todo-calendar-app",
  "version": "1.0.0",
  "description": "Todo Calendar App with React frontend and Express backend",
  "homepage": "https://sriloksagar.github.io/todo",
  "scripts": {
    "start": "cd client && npm start",
    "build": "cd client && npm run build",
    "test": "cd client && npm test",
    "eject": "cd client && npm run eject",
    "server": "cd server && npm start",
    "client": "cd client && npm start",
    "install": "cd server && npm install && cd ../client && npm install",
    "predeploy": "npm run build",
    "deploy": "gh-pages -d client/build"
  },
  "dependencies": {
    "concurrently": "^6.0.2"
  },
  "devDependencies": {
    "gh-pages": "^3.2.3"
  }
}
